-- Boxy Premium GUI Executor by ChatGPT
-- Place this LocalScript in StarterGui

local Players = game:GetService("Players")
local TeleportService = game:GetService("TeleportService")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer

-- Create ScreenGui
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "ExecutorGui"
screenGui.ResetOnSpawn = false
screenGui.Parent = LocalPlayer:WaitForChild("PlayerGui")

-- Main Frame (Box Style)
local mainFrame = Instance.new("Frame")
mainFrame.Name = "MainFrame"
mainFrame.Size = UDim2.new(0, 450, 0, 350)
mainFrame.Position = UDim2.new(0.5, -225, 0.5, -175)
mainFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 30)
mainFrame.Active = true
mainFrame.Draggable = true
mainFrame.Parent = screenGui

-- Border Only (sharp corners)
local stroke = Instance.new("UIStroke")
stroke.Thickness = 2
stroke.Color = Color3.fromRGB(50, 50, 70)
stroke.Parent = mainFrame

-- Close Button
local closeBtn = Instance.new("TextButton")
closeBtn.Name = "CloseBtn"
closeBtn.Size = UDim2.new(0, 80, 0, 30)
closeBtn.Position = UDim2.new(1, -90, 0, 10)
closeBtn.Text = "Close"
closeBtn.Font = Enum.Font.GothamBold
closeBtn.TextSize = 18
closeBtn.TextColor3 = Color3.new(1, 1, 1)
closeBtn.BackgroundColor3 = Color3.fromRGB(40, 40, 55)
closeBtn.Parent = mainFrame

local closeCorner = Instance.new("UICorner")
closeCorner.CornerRadius = UDim.new(0, 4)
closeCorner.Parent = closeBtn

closeBtn.MouseButton1Click:Connect(function()
    mainFrame.Visible = false
end)

-- Open Button
local openBtn = Instance.new("TextButton")
openBtn.Name = "OpenBtn"
openBtn.Size = UDim2.new(0, 80, 0, 30)
openBtn.Position = UDim2.new(0, 10, 0, 10)
openBtn.Text = "Open"
openBtn.Font = Enum.Font.GothamBold
openBtn.TextSize = 18
openBtn.TextColor3 = Color3.new(1, 1, 1)
openBtn.BackgroundColor3 = Color3.fromRGB(40, 40, 55)
openBtn.Parent = screenGui

local openCorner = Instance.new("UICorner")
openCorner.CornerRadius = UDim.new(0, 4)
openCorner.Parent = openBtn

openBtn.MouseButton1Click:Connect(function()
    mainFrame.Visible = not mainFrame.Visible
end)

-- Sidebar for Tabs
local sidebar = Instance.new("Frame")
sidebar.Name = "Sidebar"
sidebar.Size = UDim2.new(0, 120, 1, -50)
sidebar.Position = UDim2.new(0, 0, 0, 50)
sidebar.BackgroundTransparency = 1
sidebar.Parent = mainFrame

local tabs = {"Home","Editor","Cloud","Console","Setting","Local Scripts","Saved Scripts","Players","Serverhop","Scripts Hubs","Credits"}
local tabButtons = {}
local pages = {}
for i, tabName in ipairs(tabs) do
    local btn = Instance.new("TextButton")
    btn.Name = tabName .. "Tab"
    btn.Size = UDim2.new(1, 0, 0, 30)
    btn.Position = UDim2.new(0, 0, 0, (i-1)*30)
    btn.Text = tabName
    btn.Font = Enum.Font.Gotham
    btn.TextSize = 16
    btn.TextColor3 = Color3.fromRGB(200,200,200)
    btn.BackgroundColor3 = Color3.fromRGB(30,30,45)
    btn.Parent = sidebar
    local btnStroke = Instance.new("UIStroke") btnStroke.Thickness = 1; btnStroke.Color = Color3.fromRGB(50,50,70); btnStroke.Parent = btn
    tabButtons[tabName] = btn

    local page = Instance.new("Frame")
    page.Name = tabName .. "Page"
    page.Size = UDim2.new(1, -120, 1, -50)
    page.Position = UDim2.new(0, 120, 0, 50)
    page.BackgroundTransparency = 1
    page.Visible = (tabName == "Home")
    page.Parent = mainFrame
    pages[tabName] = page

    if not (tabName == "Home" or tabName == "Editor" or tabName == "Setting" or tabName == "Serverhop" or tabName == "Scripts Hubs" or tabName == "Credits") then
        local label = Instance.new("TextLabel")
        label.Size = UDim2.new(1, -20, 0, 50)
        label.Position = UDim2.new(0, 10, 0, 10)
        label.Text = "Coming Soon"
        label.Font = Enum.Font.Gotham
        label.TextSize = 20
        label.TextColor3 = Color3.new(1,1,1)
        label.BackgroundTransparency = 1
        label.Parent = page
    end
end
for name, btn in pairs(tabButtons) do
    btn.MouseButton1Click:Connect(function()
        for t, p in pairs(pages) do p.Visible = (t == name) end
    end)
end

-- Home: FPS Display
local fpsLabel = Instance.new("TextLabel")
fpsLabel.Size = UDim2.new(0, 200, 0, 40)
fpsLabel.Position = UDim2.new(0, 10, 0, 10)
fpsLabel.Font = Enum.Font.GothamBold
fpsLabel.TextSize = 18
fpsLabel.TextColor3 = Color3.new(1,1,1)
fpsLabel.BackgroundTransparency = 1
fpsLabel.Text = "FPS: 0"
fpsLabel.Parent = pages["Home"]

local last = tick(); local frames = 0
RunService.RenderStepped:Connect(function()
    frames += 1
    if tick() - last >= 1 then fpsLabel.Text = "FPS: " .. frames; frames = 0; last = tick() end
end)

-- Editor: Execute, Clear, Exec Clipboard, Hide/Show under clipboard
local editor = pages["Editor"]
local textBox = Instance.new("TextBox")
textBox.Name = "ScriptBox"
textBox.Size = UDim2.new(1, -40, 1, -140)
textBox.Position = UDim2.new(0, 10, 0, 10)
textBox.ClearTextOnFocus = false
textBox.Text = "-- paste script here"
textBox.Font = Enum.Font.Code
textBox.TextWrapped = true
textBox.TextYAlignment = Enum.TextYAlignment.Top
textBox.MultiLine = true
textBox.TextColor3 = Color3.new(1,1,1)
textBox.BackgroundColor3 = Color3.fromRGB(30,30,45)
textBox.Parent = editor
local boxStroke = Instance.new("UIStroke") boxStroke.Thickness = 1; boxStroke.Color = Color3.fromRGB(50,50,70); boxStroke.Parent = textBox

local btnContainer = Instance.new("Frame")
btnContainer.Size = UDim2.new(1, -20, 0, 100)
btnContainer.Position = UDim2.new(0, 10, 1, -110)
btnContainer.BackgroundTransparency = 1
btnContainer.Parent = editor

local function createBtn(name, position)
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(0, 120, 0, 40)
    btn.Position = position
    btn.Text = name
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 16
    btn.TextColor3 = Color3.new(1,1,1)
    btn.BackgroundColor3 = Color3.fromRGB(45,45,60)
    btn.Parent = btnContainer
    local bc = Instance.new("UICorner"); bc.CornerRadius = UDim.new(0,4); bc.Parent = btn
    return btn
end

local execBtn = createBtn("Execute", UDim2.new(0, 0, 0, 0))
local clipBtn = createBtn("Exec Clipboard", UDim2.new(0, 130, 0, 0))
local clearBtn = createBtn("Clear", UDim2.new(0, 260, 0, 0))
local hideBtn = createBtn("Hide", UDim2.new(0, 260, 0, 50))

execBtn.MouseButton1Click:Connect(function()
    pcall(function() loadstring(textBox.Text)() end)
end)
clearBtn.MouseButton1Click:Connect(function()
    textBox.Text = ""
end)
clipBtn.MouseButton1Click:Connect(function()
    local success, clip = pcall(getclipboard)
    if success and type(clip)=="string" then pcall(loadstring(clip)) end
end)

local hidden = false
hideBtn.MouseButton1Click:Connect(function()
    hidden = not hidden
    textBox.Visible = not hidden
    hideBtn.Text = hidden and "Show" or "Hide"
end)

-- Setting: FPS toggle and Anti Lag
local setting = pages["Setting"]
local fpsToggle = Instance.new("TextButton")
fpsToggle.Size = UDim2.new(0, 150, 0, 40)
fpsToggle.Position = UDim2.new(0, 10, 0, 10)
fpsToggle.Text = "Toggle FPS"
fpsToggle.Font = Enum.Font.GothamBold
fpsToggle.TextSize = 18
fpsToggle.TextColor3 = Color3.new(1,1,1)
fpsToggle.BackgroundColor3 = Color3.fromRGB(45,45,60)
fpsToggle.Parent = setting
local ft = Instance.new("UIStroke") ft.Thickness=1; ft.Color=Color3.fromRGB(50,50,70); ft.Parent = fpsToggle

local lagToggle = Instance.new("TextButton")
lagToggle.Size = UDim2.new(0, 150, 0, 40)
lagToggle.Position = UDim2.new(0, 10, 0, 60)
lagToggle.Text = "Toggle Anti Lag"
lagToggle.Font = Enum.Font.GothamBold
lagToggle.TextSize = 18
lagToggle.TextColor3 = Color3.new(1,1,1)
lagToggle.BackgroundColor3 = Color3.fromRGB(45,45,60)
lagToggle.Parent = setting
local lt = Instance.new("UIStroke") lt.Thickness=1; lt.Color=Color3.fromRGB(50,50,70); lt.Parent = lagToggle

local fpsOn = true
fpsToggle.MouseButton1Click:Connect(function() fpsOn = not fpsOn; fpsLabel.Visible = fpsOn end)

local lagOn = false
lagToggle.MouseButton1Click:Connect(function() lagOn = not lagOn; RunService:Set3dRenderingEnabled(not lagOn) end)

-- Serverhop and Rejoin
local srv = pages["Serverhop"]
local hopBtn = createBtn("Serverhop", UDim2.new(0, 0, 0, 0))
hopBtn.Parent = srv
local rejoinBtn = createBtn("Rejoin", UDim2.new(0, 0, 0, 50))
hopBtn.MouseButton1Click:Connect(function() TeleportService:Teleport(game.PlaceId) end)
rejoinBtn.MouseButton1Click:Connect(function() TeleportService:Teleport(game.PlaceId, LocalPlayer) end)

-- Scripts Hubs: load external script
local hub = pages["Scripts Hubs"]
local hubBtn = Instance.new("TextButton")
hubBtn.Size = UDim2.new(0, 200, 0, 40)
hubBtn.Position = UDim2.new(0, 10, 0, 10)
hubBtn.Text = "Load Script Hub"
hubBtn.Font = Enum.Font.GothamBold
hubBtn.TextSize = 18
hubBtn.TextColor3 = Color3.new(1,1,1)
hubBtn.BackgroundColor3 = Color3.fromRGB(45,45,60)
hubBtn.Parent = hub
local hb = Instance.new("UIStroke") hb.Thickness=1; hb.Color=Color3.fromRGB(50,50,70); hb.Parent = hubBtn
hubBtn.MouseButton1Click:Connect(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/godifreal/MugenUiOp/refs/heads/main/2", true))()
end)

-- Credits
local cred = pages["Credits"]
local creditLabel = Instance.new("TextLabel")
creditLabel.Size = UDim2.new(1, -20, 1, -20)
creditLabel.Position = UDim2.new(0, 10, 0, 10)
creditLabel.Text = "Executor VIP by TheMugenKing"
creditLabel.Font = Enum.Font.GothamBold
creditLabel.TextSize = 18
creditLabel.TextColor3 = Color3.new(1,1,1)
creditLabel.BackgroundTransparency = 1
creditLabel.Parent = cred
